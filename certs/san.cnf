[req]
default_bits       = 2048
prompt             = no
default_md         = sha256
req_extensions     = req_ext
distinguished_name = dn

[dn]
CN = keycloak.local

[req_ext]
subjectAltName = @alt_names

[alt_names]
DNS.1 = keycloak.local
DNS.2 = keycloak2.local
DNS.3 = app.local
DNS.4 = other.local
